{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
      "no-console": false,
      "object-literal-sort-keys": false,
      "variable-name": {
        "options": [
          "check-format",
          "allow-pascal-case",
          "allow-leading-underscore"
        ]
      }
    }
}
